{
  "name": "ugi-client",
  "version": "0.1.3",
  "description": "A client that allows for remote usage of the a ugi bot through the ugi interface.",
  "main": "index.js",
  "scripts": {
    "build": "npm-run-all build-*",
    "build-linux": "cross-var pkg -o ./dist/ugi-client-linux-$npm_package_version -t node16-linux-x64 ./index.js",
    "build-macos": "cross-var pkg -o ./dist/ugi-client-macos-$npm_package_version -t node16-macos-x64 ./index.js",
    "build-win": "cross-var pkg -o ./dist/ugi-client-win-$npm_package_version -t node16-win-x64 ./index.js",
    "dev": "node ./index.js",
    "lint": "eslint ."
  },
  "author": "",
  "license": "ISC",
  "pkg": {
    "outputPath": "dist",
    "targets": [
      "node16-linux-x64",
      "node16-macos-x64",
      "node16-win-x64"
    ]
  },
  "devDependencies": {
    "cross-var": "^1.1.0",
    "eslint": "^8.14.0",
    "npm-run-all": "^4.1.5",
    "pkg": "^5.6.0"
  },
  "dependencies": {
    "ws": "^8.5.0",
    "yargs": "^17.4.1"
  }
}